<div>
  <h1>New review</h1>
  <%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.input :rating, collection: 0..5 %>
  <%= f.input :content %>
  <%= f.submit class: "btn btn-outline-primary" %>
<% end %>
</div>

<%= link_to "Back to restaurants list", restaurants_path, class: "btn btn-outline-primary" %>
